{
  "name": "vibe-vscode",
  "displayName": "Vibe VS Code v5.0 - Multi-Agent AI Assistant",
  "description": "VIBE VS Code Extension v5.0 - Full CLI parity with Multi-Agent Orchestration, Extended Thinking, Semantic Memory, and Team Collaboration",
  "version": "5.0.1",
  "vibe-ecosystem": {
    "compatible_cli": "9.x",
    "compatible_extension": "5.x", 
    "compatible_web": "2.x",
    "cli_parity": "100%",
    "new_features": {
      "5.0.0": [
        "Multi-agent orchestration in VS Code",
        "Extended thinking for complex coding tasks", 
        "Real-time streaming with progress indicators",
        "Semantic memory search across projects",
        "Team collaboration features",
        "Advanced security scanning integration",
        "BDD test generation from VS Code",
        "Custom instructions per workspace"
      ]
    }
  },
  "publisher": "mktech",
  "repository": {
    "type": "git",
    "url": "https://github.com/mk-knight23/vibe"
  },
  "bugs": {
    "url": "https://github.com/mk-knight23/vibe/issues"
  },
  "homepage": "https://github.com/mk-knight23/vibe#readme",
  "icon": "media/vibe-icon.png",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other",
    "AI",
    "Programming Languages"
  ],
  "keywords": [
    "ai",
    "cli-parity",
    "autonomous",
    "agent",
    "coding-assistant",
    "openrouter",
    "megallm",
    "agentrouter",
    "routeway",
    "filesystem",
    "shell",
    "memory-system",
    "git-integration",
    "command-line",
    "vibe-cli",
    "mktech"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "rimraf dist && tsc -p ./tsconfig.json",
    "watch": "tsc -p ./tsconfig.json -w",
    "package": "npx @vscode/vsce package --no-dependencies",
    "test": "vitest run",
    "lint": "echo \"No lint configured\""
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/vscode": "^1.107.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.7.2",
    "@vscode/vsce": "^3.2.1",
    "vitest": "^4.0.15"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:vibe.ask",
    "onCommand:vibe.code",
    "onCommand:vibe.debug",
    "onCommand:vibe.architect",
    "onCommand:vibe.agent",
    "onCommand:vibe.runTool",
    "onCommand:vibe.showMemory",
    "onCommand:vibe.clearMemory",
    "onCommand:vibe.settings",
    "onCommand:vibe.openPanel",
    "onCommand:vibe.openFromActivityBar",
    "onCommand:vibe.switchMode",
    "onView:vibe.vibePanel"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vibe-sidebar",
          "title": "Vibe",
          "icon": "media/vibe-icon.svg"
        }
      ]
    },
    "views": {
      "vibe-sidebar": [
        {
          "id": "vibe.vibePanel",
          "type": "webview",
          "name": "Chat",
          "icon": "media/vibe-icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "vibe.openFromActivityBar",
        "title": "Vibe AI Assistant",
        "category": "Vibe",
        "icon": "$(robot)"
      },
      {
        "command": "vibe.ask",
        "title": "Vibe: Ask",
        "category": "Vibe",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "vibe.code",
        "title": "Vibe: Code",
        "category": "Vibe",
        "icon": "$(code)"
      },
      {
        "command": "vibe.debug",
        "title": "Vibe: Debug",
        "category": "Vibe",
        "icon": "$(debug)"
      },
      {
        "command": "vibe.architect",
        "title": "Vibe: Architect",
        "category": "Vibe",
        "icon": "$(circuit-board)"
      },
      {
        "command": "vibe.agent",
        "title": "Vibe: Agent",
        "category": "Vibe",
        "icon": "$(robot)"
      },
      {
        "command": "vibe.runTool",
        "title": "Vibe: Run Tool",
        "category": "Vibe",
        "icon": "$(tools)"
      },
      {
        "command": "vibe.showMemory",
        "title": "Vibe: Show Memory",
        "category": "Vibe",
        "icon": "$(database)"
      },
      {
        "command": "vibe.clearMemory",
        "title": "Vibe: Clear Memory",
        "category": "Vibe",
        "icon": "$(clear-all)"
      },
      {
        "command": "vibe.settings",
        "title": "Vibe: Settings",
        "category": "Vibe",
        "icon": "$(settings-gear)"
      },
      {
        "command": "vibe.openPanel",
        "title": "Vibe: Open Chat Panel",
        "category": "Vibe",
        "icon": "$(comment-discussion)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vibe.ask",
          "when": "editorHasSelection",
          "group": "vibe@1"
        },
        {
          "command": "vibe.code",
          "when": "editorHasSelection",
          "group": "vibe@2"
        },
        {
          "command": "vibe.debug",
          "when": "editorHasSelection",
          "group": "vibe@3"
        }
      ],
      "commandPalette": [
        {
          "command": "vibe.ask"
        },
        {
          "command": "vibe.code"
        },
        {
          "command": "vibe.debug"
        },
        {
          "command": "vibe.architect"
        },
        {
          "command": "vibe.agent"
        },
        {
          "command": "vibe.runTool"
        },
        {
          "command": "vibe.showMemory"
        },
        {
          "command": "vibe.clearMemory"
        },
        {
          "command": "vibe.settings"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Vibe v5.0 - Full CLI Parity",
      "properties": {
        "vibe.openrouterApiKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "OpenRouter API key for access to 40+ free models"
        },
        "vibe.megallmApiKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "MegaLLM API key for high-performance models"
        },
        "vibe.agentrouterApiKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "AgentRouter API key for Claude models"
        },
        "vibe.routewayApiKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "Routeway API key for specialized models"
        },
        "vibe.provider": {
          "type": "string",
          "enum": [
            "openrouter",
            "megallm",
            "agentrouter",
            "routeway"
          ],
          "default": "openrouter",
          "enumDescriptions": [
            "OpenRouter (40+ models, community-driven)",
            "MegaLLM (12 models, high performance)",
            "AgentRouter (7 models, Claude access)",
            "Routeway (6 models, specialized)"
          ],
          "markdownDescription": "Primary AI provider (automatic fallback enabled)"
        },
        "vibe.defaultModel": {
          "type": "string",
          "default": "x-ai/grok-4.1-fast:free",
          "markdownDescription": "Default model to use for AI interactions"
        },
        "vibe.executionMode": {
          "type": "string",
          "enum": [
            "ask",
            "code",
            "debug",
            "architect"
          ],
          "default": "code",
          "enumDescriptions": [
            "Ask: Q&A and explanations",
            "Code: Development and implementation",
            "Debug: Error diagnosis and fixes",
            "Architect: System design and planning"
          ],
          "markdownDescription": "Default execution mode for AI interactions"
        },
        "vibe.autoApproveUnsafeOps": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "⚠️ Auto-approve file edits and commands (DANGEROUS)"
        },
        "vibe.maxContextFiles": {
          "type": "number",
          "default": 20,
          "minimum": 1,
          "maximum": 100,
          "markdownDescription": "Maximum files to include in context"
        },
        "vibe.streamingEnabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable real-time streaming responses"
        },
        "vibe.enableMemorySystem": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable conversation memory system"
        },
        "vibe.enableDiffPreview": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Show diff previews before applying changes"
        },
        "vibe.enableProviderFallback": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Automatically fallback to other providers on failure"
        }
      }
    },
    "keybindings": [
      {
        "command": "vibe.ask",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v"
      },
      {
        "command": "vibe.agent",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a"
      }
    ]
  }
}
